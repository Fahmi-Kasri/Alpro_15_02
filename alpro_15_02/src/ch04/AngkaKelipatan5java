
public class AngkaKelipatan5 {
    public static void main(String[] args) {
        int[] kelipatan5 = new int[12];
        int index = 0;
        int nomor =1;
        
        for(int i=1; i<=82; i++){
            if(i%5==0){
            kelipatan5[index]=i;
                System.out.println(kelipatan5[index]+" ");
                nomor++;
                index++;
            }
        }
    }
}
