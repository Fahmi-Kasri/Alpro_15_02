import java.util.Scanner;

public class MencariAngka {
    public static void main(String[] args) {
        Scanner s = new Scanner(System.in);
        int n,z = 0;
        int cari;
        int[] x;
        boolean found = false;S
        
        System.out.print("input jumlah N: ");
        n = s.nextInt();
        x = new int[n];
        
        for(int index=0; index<n; index++)
        {
            System.out.println("input angka ke-" + " :");
            x[index] = s.nextInt();
    }
        System.out.println("input data yang dicari :");
        cari = s.nextInt();
        
        for(int index=0; index<n; index++)
        {
            if(x[index] == cari)

            {
                found=true;
                z=index+1;
                System.out.println("data ditemukan diindex ke : "+z);
                
            }
    
    }
if (found ==false)
{
    System.out.println("data tidak ditemukan");
}
}
    }
